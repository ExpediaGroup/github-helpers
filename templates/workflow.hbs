name: {{ spaceSeparatedCase ( titleCase helper ) }}

on:
  pull_request:
    branches: [ main ]
    paths:
      - 'src/helpers/{{ dashCase helper }}.ts'

jobs:
  test:
  runs-on: ubuntu-latest
  steps:
    - name: Checkout
      uses: actions/checkout@v4

    - uses: ./
      with:
        helper: {{ dashCase helper }}
